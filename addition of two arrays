#include<iostream>
using namespace std;
int main()
{
    int i,n1,n2,sum=0;
    cin>>n1;
    n2=n1;
    int a[n1],b[n2],c[n1];
    for(i=0;i<n1;i++)
    {
        cin>>a[i];
    }
    for(i=0;i<n2;i++)
    {
        cin>>b[i];
    }
    for(int j=0;j<n1;j++)
    {
        int d=a[j]+b[j];
        c[j]=d;
    }
    for(i=0;i<n1;i++)
    {
        cout<<c[i]<<" ";
    }
}
