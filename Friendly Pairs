#include<iostream>
using namespace std;
int main()
{
    int a,b,i,count1=0,count2=0;
    cin>>a>>b;
    for(i=1;i<=a;i++)
    {
        if(a%i==0)
        {
            count1+=i;
        }
    }
    for(i=1;i<=b;i++)
    {
        if(b%i==0)
        {
            count2+=i;
        }
    }
    if((count1/a)==(count2/b))
    {
        cout<<"Friendly Pair";
    }
    else
    {
        cout<<"Not Friendly Pair";
    }
}
