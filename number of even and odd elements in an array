#include<iostream>
using namespace std;
int main()
{
    int n,count1=0,count2=0;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
        if(a[i]%2==1)
        {
            count1+=1;
        }
        else
        {
            count2+=1;
        }
    }
    cout<<"Odd: "<<count1<<endl<<"Even: "<<count2;
}
