#include<iostream>
using namespace std;
int main()
{
    int n,val,count=0;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cin>>val;
    for(int i=0;i<n;i++)
    {
        if(a[i]==val)
        {
            count+=1;
        }

    }
    if(count>=1)
    {
        cout<<val<<" is present in the array";
    }
    else
    {
        cout<<val<<" is not present in the array";
    }
    
}
