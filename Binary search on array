#include<iostream>
using namespace std;
int main()
{
    int n,pos=0;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int val;
    cin>>val;
    for(int i=0;i<n;i++)
    {
        if(a[i]==val)
        {
            pos=i+1;
        }
    }
    if(pos>=1)
    {
        cout<<"Number present at index "<<pos;
    }
    else
    {
        cout<<"The number is not present in the list";
    }
}
