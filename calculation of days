#include<iostream>
using namespace std;
int main()
{
    int mon,year;
    cin>>mon>>year;
    if(mon==1 || mon==3 || mon==5 || mon==7 || mon==8 || mon==10 || mon==12)
    {
        cout<<"Number of days is 31";
    }
    else if(mon==2)
    {
        if(year%4==0)
        {
            cout<<"Number of days is 29";
        }
        else
        {
            cout<<"Number of days is 28";
        }
    }
    else
    {
        cout<<"Number of days is 30";
    }
}
