#include<iostream>
using namespace std;
int main()
{
    int n1,n2,sum1=0,sum2=0;
    cin>>n1>>n2;
    int a[n1],b[n2];
    for(int i=0;i<n1;i++)
    {
        cin>>a[i];
        sum1+=a[i];
    }
    for(int j=0;j<n2;j++)
    {
        cin>>a[j];
        sum2+=a[j];
    }
    if(sum1==sum2 && n1==n2)
    {
        cout<<"Same";
    }
    else
    {
        cout<<"Not Same";
    }
}
