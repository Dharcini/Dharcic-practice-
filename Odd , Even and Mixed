#include<iostream>
using namespace std;
int main()
{
    int n,c1=0,c2=0;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
        if(a[i]%2==1)
        {
            c1+=1;
        }
        else
        {
            c2+=1;
        }
    }
    if(c1==n)
    {
        cout<<"Odd";
    }
    else if(c2==n)
    {
        cout<<"Even";
    }
    else
    {
        cout<<"Mixed";
    }
}
